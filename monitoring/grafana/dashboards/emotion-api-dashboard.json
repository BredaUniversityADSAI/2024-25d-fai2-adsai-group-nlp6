{
  "id": null,
  "uid": "unified-emotion-dashboard",
  "title": "ðŸš€ Emotion Classification API - Complete Monitoring Dashboard",
  "description": "Comprehensive monitoring dashboard for Emotion Classification API including ML model performance, system metrics, and operational insights",
  "tags": [
    "emotion-classification",
    "machine-learning",
    "api-monitoring",
    "system-metrics",
    "production"
  ],
  "style": "dark",
  "timezone": "browser",
  "refresh": "30s",
  "time": {
    "from": "now-1h",
    "to": "now"
  },
  "timepicker": {
    "refresh_intervals": ["5s", "10s", "30s", "1m", "5m", "15m", "30m", "1h", "2h", "1d"]
  },
  "panels": [
    {
      "id": 100,
      "title": "ðŸ“Š Overview & Key Metrics",
      "type": "row",
      "collapsed": false,
      "gridPos": {
        "h": 1,
        "w": 24,
        "x": 0,
        "y": 0
      }
    },
    {
      "id": 1,
      "title": "ðŸŒŸ API Status",
      "type": "stat",
      "targets": [
        {
          "expr": "up{job=\"emotion-api\"}",
          "legendFormat": "API Status",
          "refId": "A"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "color": {
            "mode": "thresholds"
          },
          "mappings": [
            {
              "options": {
                "0": {
                  "text": "DOWN",
                  "color": "red"
                },
                "1": {
                  "text": "UP",
                  "color": "green"
                }
              },
              "type": "value"
            }
          ],
          "thresholds": {
            "steps": [
              {
                "color": "red",
                "value": null
              },
              {
                "color": "green",
                "value": 1
              }
            ]
          },
          "unit": "none"
        }
      },
      "gridPos": {
        "h": 4,
        "w": 6,
        "x": 0,
        "y": 1
      }
    },
    {
      "id": 2,
      "title": "ðŸŽ¯ Current Model Accuracy",
      "type": "stat",
      "targets": [
        {
          "expr": "emotion_model_accuracy",
          "legendFormat": "Accuracy",
          "refId": "A"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "color": {
            "mode": "thresholds"
          },
          "mappings": [],
          "thresholds": {
            "steps": [
              {
                "color": "red",
                "value": null
              },
              {
                "color": "yellow",
                "value": 0.8
              },
              {
                "color": "green",
                "value": 0.85
              }
            ]
          },
          "unit": "percentunit",
          "min": 0,
          "max": 1
        }
      },
      "gridPos": {
        "h": 4,
        "w": 6,
        "x": 6,
        "y": 1
      }
    },
    {
      "id": 3,
      "title": "âš¡ Request Rate",
      "type": "stat",
      "targets": [
        {
          "expr": "rate(http_requests_total[5m])",
          "legendFormat": "Requests/sec",
          "refId": "A"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "color": {
            "mode": "palette-classic"
          },
          "mappings": [],
          "thresholds": {
            "steps": [
              {
                "color": "green",
                "value": null
              },
              {
                "color": "yellow",
                "value": 5
              },
              {
                "color": "red",
                "value": 10
              }
            ]
          },
          "unit": "reqps"
        }
      },
      "gridPos": {
        "h": 4,
        "w": 6,
        "x": 12,
        "y": 1
      }
    },
    {
      "id": 4,
      "title": "ðŸš¨ Error Rate",
      "type": "stat",
      "targets": [
        {
          "expr": "rate(http_request_exceptions_total[5m]) / rate(http_requests_total[5m]) * 100",
