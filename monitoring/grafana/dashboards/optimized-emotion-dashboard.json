{
  "id": null,
  "uid": "optimized-emotion-dashboard",
  "title": "üöÄ Emotion Classification API - Optimized Dashboard",
  "description": "Streamlined monitoring dashboard focusing on critical ML performance and operational metrics",
  "tags": [
    "emotion-classification",
    "machine-learning",
    "api-monitoring",
    "production"
  ],
  "style": "dark",
  "timezone": "browser",
  "refresh": "30s",
  "time": {
    "from": "now-1h",
    "to": "now"
  },
  "timepicker": {
    "refresh_intervals": ["5s", "10s", "30s", "1m", "5m", "15m", "30m", "1h", "2h", "1d"]
  },
  "templating": {
    "list": []
  },
  "annotations": {
    "list": []
  },
  "panels": [
    {
      "id": 100,
      "title": "üìä System Resources & Performance",
      "type": "row",
      "collapsed": false,
      "gridPos": {
        "h": 1,
        "w": 24,
        "x": 0,
        "y": 0
      }
    },
    {
      "id": 1,
      "title": "üñ•Ô∏è Backend CPU Usage",
      "type": "stat",
      "targets": [
        {
          "expr": "rate(container_cpu_usage_seconds_total{name=\"emotion_backend\"}[5m]) * 100",
          "legendFormat": "CPU %",
          "refId": "A"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "color": {
            "mode": "thresholds"
          },
          "thresholds": {
            "steps": [
              {
                "color": "green",
                "value": null
              },
              {
                "color": "yellow",
                "value": 70
              },
              {
                "color": "red",
                "value": 90
              }
            ]
          },
          "unit": "percent",
          "min": 0,
          "max": 100
        }
      },
      "gridPos": {
        "h": 6,
        "w": 6,
        "x": 0,
        "y": 1
      }
    },
    {
      "id": 2,
      "title": "üíæ Backend Memory Usage",
      "type": "stat", 
      "targets": [
        {
          "expr": "(container_memory_usage_bytes{name=\"emotion_backend\"} / container_spec_memory_limit_bytes{name=\"emotion_backend\"}) * 100",
          "legendFormat": "Memory %",
          "refId": "A"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "color": {
            "mode": "thresholds"
          },
          "thresholds": {
            "steps": [
              {
                "color": "green",
                "value": null
              },
              {
                "color": "yellow",
                "value": 80
              },
              {
                "color": "red",
                "value": 95
              }
            ]
          },
          "unit": "percent",
          "min": 0,
          "max": 100
        }
      },
      "gridPos": {
        "h": 6,
        "w": 6,
        "x": 6,
        "y": 1
      }
    },
    {
      "id": 3,
      "title": "üìà All Containers Resource Usage",
      "type": "timeseries",
      "targets": [
        {
          "expr": "rate(container_cpu_usage_seconds_total{name=~\"emotion_.*\"}[5m]) * 100",
          "legendFormat": "{{name}} CPU",
          "refId": "A"
        },
        {
          "expr": "(container_memory_usage_bytes{name=~\"emotion_.*\"} / 1024/1024)",
          "legendFormat": "{{name}} Memory MB", 
          "refId": "B"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "color": {
            "mode": "palette-classic"
          }
        },
        "overrides": [
          {
            "matcher": {
              "id": "byRegexp",
              "options": ".*CPU.*"
            },
            "properties": [
              {
                "id": "unit",
                "value": "percent"
              }
            ]
          },
          {
            "matcher": {
              "id": "byRegexp", 
              "options": ".*Memory.*"
            },
            "properties": [
              {
                "id": "unit",
                "value": "decbytes"
              }
            ]
          }
        ]
      },
      "gridPos": {
        "h": 6,
        "w": 12,
        "x": 12,
        "y": 1
      }
    },
    {
      "id": 200,
      "title": "ü§ñ Model Performance Metrics",
      "type": "row",
      "collapsed": false,
      "gridPos": {
        "h": 1,
        "w": 24,
        "x": 0,
        "y": 7
      }
    },
    {
      "id": 5,
      "title": "üìà Model F1 Scores by Task",
      "type": "stat",
      "targets": [
        {
          "expr": "model_f1_score",
          "legendFormat": "{{task}}",
          "refId": "A"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "color": {
            "mode": "thresholds"
          },
          "thresholds": {
            "steps": [
              {
                "color": "red",
                "value": null
              },
              {
                "color": "yellow",
                "value": 0.6
              },
              {
                "color": "green",
                "value": 0.8
              }
            ]
          },
          "unit": "none",
          "min": 0,
          "max": 1,
          "decimals": 3
        }
      },
      "gridPos": {
        "h": 8,
        "w": 12,
        "x": 0,
        "y": 8
      }
    },
    {
      "id": 6,
      "title": "üéØ Drift Detection Status",
      "type": "timeseries",
      "targets": [
        {
          "expr": "data_drift_score",
          "legendFormat": "Data Drift",
          "refId": "A"
        },
        {
          "expr": "concept_drift_score",
          "legendFormat": "Concept Drift",
          "refId": "B"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "color": {
            "mode": "palette-classic"
          },
          "unit": "none",
          "min": 0,
          "max": 1
        }
      },
      "gridPos": {
        "h": 8,
        "w": 12,
        "x": 12,
        "y": 8
      }
    },
    {
      "id": 300,
      "title": "‚ö° Latency & Performance Tracking",
      "type": "row",
      "collapsed": false,
      "gridPos": {
        "h": 1,
        "w": 24,
        "x": 0,
        "y": 16
      }
    },
    {
      "id": 7,
      "title": "üé§ Transcription Latency",
      "type": "timeseries",
      "targets": [
        {
          "expr": "histogram_quantile(0.95, rate(transcription_latency_seconds_bucket[5m]))",
          "legendFormat": "95th percentile",
          "refId": "A"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "s",
          "color": {
            "mode": "palette-classic"
          }
        }
      },
      "gridPos": {
        "h": 8,
        "w": 8,
        "x": 0,
        "y": 17
      }
    },
    {
      "id": 8,
      "title": "üß† Prediction Latency",
      "type": "timeseries",
      "targets": [
        {
          "expr": "histogram_quantile(0.95, rate(emotion_prediction_latency_seconds_bucket[5m]))",
          "legendFormat": "95th percentile",
          "refId": "A"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "s",
          "color": {
            "mode": "palette-classic"
          }
        }
      },
      "gridPos": {
        "h": 8,
        "w": 8,
        "x": 8,
        "y": 17
      }
    },
    {
      "id": 9,
      "title": "üìä Model Confidence",
      "type": "timeseries",
      "targets": [
        {
          "expr": "rate(model_confidence_distribution_bucket[5m])",
          "legendFormat": "Confidence Rate",
          "refId": "A"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "ops",
          "color": {
            "mode": "palette-classic"
          }
        }
      },
      "gridPos": {
        "h": 8,
        "w": 8,
        "x": 16,
        "y": 17
      }
    },
    {
      "id": 400,
      "title": "üö® API Operations & Monitoring",
      "type": "row",
      "collapsed": false,
      "gridPos": {
        "h": 1,
        "w": 24,
        "x": 0,
        "y": 25
      }
    },
    {
      "id": 10,
      "title": "‚ö° Request Rate (per minute)",
      "type": "stat",
      "targets": [
        {
          "expr": "rate(emotion_predictions_total[1m]) * 60",
          "legendFormat": "Requests/min",
          "refId": "A"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "color": {
            "mode": "thresholds"
          },
          "thresholds": {
            "steps": [
              {
                "color": "green",
                "value": null
              },
              {
                "color": "yellow",
                "value": 100
              },
              {
                "color": "red",
                "value": 500
              }
            ]
          },
          "unit": "none"
        }
      },
      "gridPos": {
        "h": 6,
        "w": 6,
        "x": 0,
        "y": 26
      }
    },
    {
      "id": 11,
      "title": "üö® Error Rate (%)",
      "type": "stat",
      "targets": [
        {
          "expr": "(rate(api_errors_total[5m]) / rate(emotion_predictions_total[5m])) * 100",
          "legendFormat": "Error Rate",
          "refId": "A"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "color": {
            "mode": "thresholds"
          },
          "thresholds": {
            "steps": [
              {
                "color": "green",
                "value": null
              },
              {
                "color": "yellow",
                "value": 1
              },
              {
                "color": "red",
                "value": 5
              }
            ]
          },
          "unit": "percent"
        }
      },
      "gridPos": {
        "h": 6,
        "w": 6,
        "x": 6,
        "y": 26
      }
    },
    {
      "id": 12,
      "title": "üîç Active Requests",
      "type": "stat",
      "targets": [
        {
          "expr": "active_requests_count",
          "legendFormat": "Active",
          "refId": "A"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "color": {
            "mode": "thresholds"
          },
          "thresholds": {
            "steps": [
              {
                "color": "green",
                "value": null
              },
              {
                "color": "yellow",
                "value": 10
              },
              {
                "color": "red",
                "value": 50
              }
            ]
          },
          "unit": "none"
        }
      },
      "gridPos": {
        "h": 6,
        "w": 6,
        "x": 12,
        "y": 26
      }
    },
    {
      "id": 13,
      "title": "üìà API Errors Over Time",
      "type": "timeseries",
      "targets": [
        {
          "expr": "rate(api_errors_total[5m])",
          "legendFormat": "{{error_type}} - {{endpoint}}",
          "refId": "A"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "ops"
        }
      },
      "gridPos": {
        "h": 6,
        "w": 6,
        "x": 18,
        "y": 26
      }
    }
  ],
  "version": 1
}
