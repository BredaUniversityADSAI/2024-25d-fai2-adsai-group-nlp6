{
  "id": null,
  "uid": "final-emotion-dashboard",
  "title": "üéØ Emotion Classification API - Unified Monitoring Dashboard",
  "description": "Complete monitoring solution for Emotion Classification API with optimized metrics and visualizations",
  "tags": [
    "emotion-classification",
    "machine-learning",
    "api-monitoring",
    "production",
    "unified"
  ],
  "style": "dark",
  "timezone": "browser",
  "refresh": "30s",
  "time": {
    "from": "now-1h",
    "to": "now"
  },
  "timepicker": {
    "refresh_intervals": ["5s", "10s", "30s", "1m", "5m", "15m", "30m", "1h", "2h", "1d"]
  },
  "templating": {
    "list": []
  },
  "annotations": {
    "list": []
  },
  "panels": [
    {
      "id": 100,
      "title": "üñ•Ô∏è System Resources & Container Health",
      "type": "row",
      "collapsed": false,
      "gridPos": {
        "h": 1,
        "w": 24,
        "x": 0,
        "y": 0
      }
    },
    {
      "id": 1,
      "title": "Backend Container CPU Usage",
      "type": "stat",
      "targets": [
        {
          "expr": "rate(container_cpu_usage_seconds_total{container_label_com_docker_compose_service=\"backend\"}[5m]) * 100",
          "legendFormat": "CPU %",
          "refId": "A"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "percent",
          "thresholds": {
            "steps": [
              {
                "color": "green",
                "value": null
              },
              {
                "color": "yellow",
                "value": 70
              },
              {
                "color": "red",
                "value": 90
              }
            ]
          }
        }
      },
      "gridPos": {
        "h": 6,
        "w": 6,
        "x": 0,
        "y": 1
      }
    },
    {
      "id": 2,
      "title": "Backend Container Memory Usage",
      "type": "stat",
      "targets": [
        {
          "expr": "container_memory_usage_bytes{container_label_com_docker_compose_service=\"backend\"} / container_spec_memory_limit_bytes{container_label_com_docker_compose_service=\"backend\"} * 100",
          "legendFormat": "Memory %",
          "refId": "A"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "percent",
          "thresholds": {
            "steps": [
              {
                "color": "green",
                "value": null
              },
              {
                "color": "yellow",
                "value": 80
              },
              {
                "color": "red",
                "value": 95
              }
            ]
          }
        }
      },
      "gridPos": {
        "h": 6,
        "w": 6,
        "x": 6,
        "y": 1
      }
    },
    {
      "id": 3,
      "title": "Container Resource Trends",
      "type": "timeseries",
      "targets": [
        {
          "expr": "rate(container_cpu_usage_seconds_total{container_label_com_docker_compose_service=\"backend\"}[5m]) * 100",
          "legendFormat": "CPU %",
          "refId": "A"
        },
        {
          "expr": "container_memory_usage_bytes{container_label_com_docker_compose_service=\"backend\"} / 1024 / 1024",
          "legendFormat": "Memory MB",
          "refId": "B"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "short"
        },
        "overrides": [
          {
            "matcher": {
              "id": "byName",
              "options": "Memory MB"
            },
            "properties": [
              {
                "id": "unit",
                "value": "decbytes"
              }
            ]
          }
        ]
      },
      "gridPos": {
        "h": 6,
        "w": 12,
        "x": 12,
        "y": 1
      }
    },
    {
      "id": 200,
      "title": "ü§ñ Model Performance & Quality",
      "type": "row",
      "collapsed": false,
      "gridPos": {
        "h": 1,
        "w": 24,
        "x": 0,
        "y": 7
      }
    },
    {
      "id": 4,
      "title": "Model F1 Score by Task",
      "type": "stat",
      "targets": [
        {
          "expr": "model_f1_score{task=\"emotion\"}",
          "legendFormat": "Emotion F1",
          "refId": "A"
        },
        {
          "expr": "model_f1_score{task=\"sub_emotion\"}",
          "legendFormat": "Sub-Emotion F1",
          "refId": "B"
        },
        {
          "expr": "model_f1_score{task=\"intensity\"}",
          "legendFormat": "Intensity F1",
          "refId": "C"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "short",
          "decimals": 3,
          "thresholds": {
            "steps": [
              {
                "color": "red",
                "value": null
              },
              {
                "color": "yellow",
                "value": 0.5
              },
              {
                "color": "green",
                "value": 0.7
              }
            ]
          }
        }
      },
      "gridPos": {
        "h": 6,
        "w": 8,
        "x": 0,
        "y": 8
      }
    },
    {
      "id": 5,
      "title": "Model Confidence Distribution",
      "type": "histogram",
      "targets": [
        {
          "expr": "rate(model_confidence_distribution_bucket[5m])",
          "legendFormat": "Confidence {{le}}",
          "refId": "A"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "short"
        }
      },
      "gridPos": {
        "h": 6,
        "w": 8,
        "x": 8,
        "y": 8
      }
    },
    {
      "id": 6,
      "title": "Data Drift Detection",
      "type": "stat",
      "targets": [
        {
          "expr": "data_drift_score",
          "legendFormat": "Data Drift",
          "refId": "A"
        },
        {
          "expr": "concept_drift_score",
          "legendFormat": "Concept Drift",
          "refId": "B"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "short",
          "decimals": 4,
          "thresholds": {
            "steps": [
              {
                "color": "green",
                "value": null
              },
              {
                "color": "yellow",
                "value": 0.05
              },
              {
                "color": "red",
                "value": 0.1
              }
            ]
          }
        }
      },
      "gridPos": {
        "h": 6,
        "w": 8,
        "x": 16,
        "y": 8
      }
    },
    {
      "id": 300,
      "title": "‚è±Ô∏è Performance & Latency",
      "type": "row",
      "collapsed": false,
      "gridPos": {
        "h": 1,
        "w": 24,
        "x": 0,
        "y": 14
      }
    },
    {
      "id": 7,
      "title": "üéµ Transcription Latency",
      "type": "timeseries",
      "targets": [
        {
          "expr": "histogram_quantile(0.50, rate(transcription_latency_seconds_bucket[5m]))",
          "legendFormat": "50th percentile",
          "refId": "A"
        },
        {
          "expr": "histogram_quantile(0.95, rate(transcription_latency_seconds_bucket[5m]))",
          "legendFormat": "95th percentile",
          "refId": "B"
        },
        {
          "expr": "histogram_quantile(0.99, rate(transcription_latency_seconds_bucket[5m]))",
          "legendFormat": "99th percentile",
          "refId": "C"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "s"
        }
      },
      "gridPos": {
        "h": 8,
        "w": 12,
        "x": 0,
        "y": 15
      }
    },
    {
      "id": 8,
      "title": "üß† Prediction Latency",
      "type": "timeseries",
      "targets": [
        {
          "expr": "histogram_quantile(0.50, rate(emotion_prediction_latency_seconds_bucket[5m]))",
          "legendFormat": "50th percentile",
          "refId": "A"
        },
        {
          "expr": "histogram_quantile(0.95, rate(emotion_prediction_latency_seconds_bucket[5m]))",
          "legendFormat": "95th percentile",
          "refId": "B"
        },
        {
          "expr": "histogram_quantile(0.99, rate(emotion_prediction_latency_seconds_bucket[5m]))",
          "legendFormat": "99th percentile",
          "refId": "C"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "s"
        }
      },
      "gridPos": {
        "h": 8,
        "w": 12,
        "x": 12,
        "y": 15
      }
    },
    {
      "id": 400,
      "title": "üåê API Operations & Health",
      "type": "row",
      "collapsed": false,
      "gridPos": {
        "h": 1,
        "w": 24,
        "x": 0,
        "y": 23
      }
    },
    {
      "id": 9,
      "title": "‚ö° Request Rate",
      "type": "stat",
      "targets": [
        {
          "expr": "rate(emotion_predictions_total[5m]) * 60",
          "legendFormat": "Predictions/min",
          "refId": "A"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "reqps",
          "decimals": 1,
          "thresholds": {
            "steps": [
              {
                "color": "green",
                "value": null
              },
              {
                "color": "yellow",
                "value": 10
              },
              {
                "color": "red",
                "value": 50
              }
            ]
          }
        }
      },
      "gridPos": {
        "h": 6,
        "w": 6,
        "x": 0,
        "y": 24
      }
    },
    {
      "id": 10,
      "title": "üö® Error Rate",
      "type": "stat",
      "targets": [
        {
          "expr": "rate(api_errors_total[5m]) / rate(emotion_predictions_total[5m]) * 100",
          "legendFormat": "Error %",
          "refId": "A"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "percent",
          "decimals": 2,
          "thresholds": {
            "steps": [
              {
                "color": "green",
                "value": null
              },
              {
                "color": "yellow",
                "value": 1
              },
              {
                "color": "red",
                "value": 5
              }
            ]
          }
        }
      },
      "gridPos": {
        "h": 6,
        "w": 6,
        "x": 6,
        "y": 24
      }
    },
    {
      "id": 11,
      "title": "üîç Active Requests",
      "type": "stat",
      "targets": [
        {
          "expr": "active_requests_count",
          "legendFormat": "Active",
          "refId": "A"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "short",
          "thresholds": {
            "steps": [
              {
                "color": "green",
                "value": null
              },
              {
                "color": "yellow",
                "value": 5
              },
              {
                "color": "red",
                "value": 20
              }
            ]
          }
        }
      },
      "gridPos": {
        "h": 6,
        "w": 6,
        "x": 12,
        "y": 24
      }
    },
    {
      "id": 12,
      "title": "üìà API Health Trends",
      "type": "timeseries",
      "targets": [
        {
          "expr": "rate(emotion_predictions_total[5m]) * 60",
          "legendFormat": "Requests/min",
          "refId": "A"
        },
        {
          "expr": "rate(api_errors_total[5m]) * 60",
          "legendFormat": "Errors/min",
          "refId": "B"
        },
        {
          "expr": "active_requests_count",
          "legendFormat": "Active Requests",
          "refId": "C"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "short"
        },
        "overrides": [
          {
            "matcher": {
              "id": "byName",
              "options": "Errors/min"
            },
            "properties": [
              {
                "id": "color",
                "value": {
                  "mode": "fixed",
                  "fixedColor": "red"
                }
              }
            ]
          }
        ]
      },
      "gridPos": {
        "h": 6,
        "w": 6,
        "x": 18,
        "y": 24
      }
    },
    {
      "id": 500,
      "title": "üìä Emotion Analysis Insights",
      "type": "row",
      "collapsed": false,
      "gridPos": {
        "h": 1,
        "w": 24,
        "x": 0,
        "y": 30
      }
    },
    {
      "id": 13,
      "title": "Top Emotions Detected",
      "type": "piechart",
      "targets": [
        {
          "expr": "topk(7, increase(emotion_predictions_total[1h]))",
          "legendFormat": "{{emotion}}",
          "refId": "A"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "short"
        }
      },
      "gridPos": {
        "h": 8,
        "w": 8,
        "x": 0,
        "y": 31
      }
    },
    {
      "id": 14,
      "title": "Intensity Distribution",
      "type": "piechart",
      "targets": [
        {
          "expr": "sum by (intensity) (increase(emotion_predictions_total[1h]))",
          "legendFormat": "{{intensity}}",
          "refId": "A"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "short"
        }
      },
      "gridPos": {
        "h": 8,
        "w": 8,
        "x": 8,
        "y": 31
      }
    },
    {
      "id": 15,
      "title": "Predictions Over Time",
      "type": "timeseries",
      "targets": [
        {
          "expr": "increase(emotion_predictions_total[5m])",
          "legendFormat": "{{emotion}} - {{intensity}}",
          "refId": "A"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "short"
        }
      },
      "gridPos": {
        "h": 8,
        "w": 8,
        "x": 16,
        "y": 31
      }
    }
  ],
  "version": 1
}
